{
    "name": "oro/commerce-dpd",
    "description": "OroCommerce DPD Integration",
    "homepage": "https://github.com/orocommerce/OroDPDBundle.git",
    "license": "OSL-3.0",
    "keywords": ["Oro", "DPD", "Integration"],
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocommerce.com"
        }
    ],
    "autoload": {
        "psr-4": {"Oro\\Bundle\\DPDBundle\\": ""},
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "php": "7.1.*",
        "oro/commerce": "3.0.x-dev"
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
             "dev-3.0-beta": "3.0.x-dev"
        }
    }
}
